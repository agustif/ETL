{
  "name": "etl",
  "version": "0.0.1",
  "description": "A simple framework for building ETL Pipelines",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agustif/ETL.git"
  },
  "author": "Agusti Fernandez Pardo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agustif/ETL/issues"
  },
  "homepage": "https://github.com/agustif/ETL#readme",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node-fetch": "^2.5.10",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn build"
    }
  }
}